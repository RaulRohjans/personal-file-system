<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.TextField?>
<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="125.0"
            prefWidth="505.0" xmlns="http://javafx.com/javafx/21.0.1" xmlns:fx="http://javafx.com/fxml/1"
            fx:id="numSelectorRoot" fx:controller="com.rohjans.controllers.NumberRangeSelectorController">
   <bottom>
      <ButtonBar prefHeight="40.0" prefWidth="200.0" style="-fx-padding: 4;" BorderPane.alignment="CENTER">
        <buttons>
          <Button mnemonicParsing="false" text="Cancel" onAction="#onCancel" />
            <Button mnemonicParsing="false" text="Ok" onAction="#onOk"/>
        </buttons>
      </ButtonBar>
   </bottom>
   <center>
      <HBox prefHeight="76.0" prefWidth="565.0" style="-fx-alignment: center; -fx-padding: 8;"
            BorderPane.alignment="CENTER">
         <VBox style="-fx-padding: 0 8 0 0;">
            <Label text="From"/>
            <TextField prefHeight="26.0" prefWidth="211.0" style="-fx-min-width: 225;" fx:id="txtFrom"/>
         </VBox>
         <VBox style="-fx-padding: 0 0 0 8;">
            <Label text="To"/>
            <TextField prefHeight="26.0" prefWidth="217.0" style="-fx-min-width: 225;" fx:id="txtTo"/>
         </VBox>
      </HBox>
   </center>
</BorderPane>
