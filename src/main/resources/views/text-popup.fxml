<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<VBox prefHeight="121.0" prefWidth="363.0" style="-fx-padding: 8;" xmlns="http://javafx.com/javafx/21.0.1"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.rohjans.controllers.TextPopupController"
      onKeyReleased="#onKeyRelease" fx:id="popupRoot">
   <children>
      <HBox style="-fx-alignment: center;">
         <children>
            <VBox prefHeight="360.0" prefWidth="423.0" style="-fx-alignment: center-left;">
               <children>
                  <Label style="-fx-padding: 0 0 4 0;" text="Name:" fx:id="lblText" />
                  <TextField fx:id="txtText" />
               </children>
            </VBox>
         </children>
      </HBox>
      <ButtonBar prefHeight="40.0" prefWidth="200.0">
        <buttons>
          <Button mnemonicParsing="false" text="Cancel" fx:id="btnCancel" onAction="#onCancel" />
          <Button mnemonicParsing="false" text="Ok" onAction="#onOk" />
        </buttons>
      </ButtonBar>
   </children>
</VBox>
